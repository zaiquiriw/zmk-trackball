/ {
    behaviors {
        mod_morph: mod_morph {
            compatible = "zmk,behavior-mod-morph";
            label = "MOD_MORPH_TEST";
            #binding-cells = <0>;
            bindings = <&kp A>, <&kp B>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
    };

	keymap {
		compatible = "zmk,keymap";
		label ="Default keymap";

		default_layer {
			bindings = <
				&kp LEFT_ALT &mod_morph
				&kp LEFT_SHIFT &kp RIGHT_SHIFT
			>;
		};
	};
};
